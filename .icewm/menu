prog "Terminal urxvt" utilities-terminal urxvtc-256color
prog "Firefox" firefox xdg-open about:blank
prog "Google Chrome" google-chrome google-chrome
prog Geany geany geany
prog "pgAdmin" pgadmin3 pgadmin3 
#prog Nautilus utilities-file-manager nautilus 
prog "PCManFM" system-file-manager pcmanfm 
