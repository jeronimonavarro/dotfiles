# This is an example for IceWM's toolbar definition file.
#
# Place your variants in @CFGDIR@ or in $HOME/.icewm
# since modifications to this file will be discarded when you
# (re)install icewm.
#
menu "Favoritos" settings-default-applications {
	prog "Terminal urxvt" utilities-terminal urxvtc-256color
	prog "Firefox" firefox xdg-open about:blank
	prog "Google Chrome" google-chrome google-chrome
	prog Geany geany geany
	prog Nautilus utilities-file-manager nautilus 
   prog "PCManFM" utilities-file-manager pcmanfm 
	runonce "Oracle VM VirtualBox" system-virtualbox ".VirtualBox" VirtualBox 
   prog Gimp gimp gimp
}
